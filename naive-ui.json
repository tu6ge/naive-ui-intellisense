{
  "NButton": {
    "name": "NButton",
    "description": "按钮组件，用于触发操作。",
    "doc": {
      "summary": "用于触发一个即时操作。",
      "since": "2.0.0"
    },
    "props": {
      "type": {
        "type": "string",
        "values": ["default", "primary", "success", "warning", "error"],
        "default": "default",
        "description": "按钮类型"
      },
      "size": {
        "type": "string",
        "values": ["small", "medium", "large"],
        "default": "medium",
        "description": "按钮尺寸"
      },
      "disabled": {
        "type": "boolean",
        "default": false,
        "description": "是否禁用"
      }
    }
  },

  "NInput": {
    "name": "NInput",
    "description": "输入框组件。",
    "doc": {
      "summary": "用于文本输入。",
      "since": "2.0.0"
    },
    "props": {
      "value": {
        "type": "string",
        "description": "输入值"
      },
      "clearable": {
        "type": "boolean",
        "default": false,
        "description": "是否可清空"
      }
    }
  }
}
